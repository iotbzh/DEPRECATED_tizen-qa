  {
    "name": "template-manager",
    "exec_type": "manual",
    "priority": "3",
    "status": "designed",
    "type": "functional_positive",
    "subtype": "",
    "objective": "Test the new template manager",
    "description": "Check if the new template manager provided by LibreOffice 4 works correctly",
    "pre_condition": "Have LibreOffice StartCenter open",
    "steps": [
      {
        "description": "Click on the \"Templates...\" button",
        "expected": "A \"Template Manager window opens"
      },
      {
        "description": "Clicl on the \"Documents\" tab",
        "expected": "A new tab appears"
      },
      {
        "description": "Select one of the Templates proposed",
        "expected": "Writer starts and the document template is successfully loaded"
      },
      {
        "description": "Close Writer",
        "expected": ""
      },
      {
        "description": "Open LibreOffice StartCenter",
        "expected": ""
      },
      {
        "description": "Click on the \"Templates...\" button",
        "expected": "A \"Template Manager window opens"
      },
      {
        "description": "Click on the \"Spreadsheets\" tab",
        "expected": "A new tab appears"
      },
      {
        "description": "Select one of the Templates proposed",
        "expected": "Calc starts and the spreadsheet template is successfully loaded"
      },
      {
        "description": "Close Calc",
        "expected": ""
      },
      {
        "description": "Open LibreOffice StartCenter",
        "expected": ""
      },
      {
        "description": "Click on the \"Templates...\" button",
        "expected": "A \"Template Manager window opens"
      },
      {
        "description": "Click on the \"Presentations\" tab",
        "expected": "A new tab appears"
      },
      {
        "description": "Select one of the Templates proposed",
        "expected": "Impress starts and the presentation template is successfully loaded"
      }
    ],
    "post_condition": "",
    "notes": "",
