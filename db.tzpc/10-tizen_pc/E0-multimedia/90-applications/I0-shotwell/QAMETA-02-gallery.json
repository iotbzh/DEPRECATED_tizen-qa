  {
    "name": "rotate-gallery",
    "exec_type": "manual",
    "priority": "4",
    "status": "designed",
    "type": "functional_positive",
    "subtype": "",
    "objective": "Pictures of the gallery can be rotated",
    "description": "Check that pictures can be rotated",
    "pre_condition": "have image files imported in the gallery",
    "steps": [
      {
        "description": "open shotwell",
        "expected": "shotwell opens in \"gallery mode\""
      },
      {
        "description": "select an image of the galery",
        "expected": "the edit buttons are displayed"
      },
      {
        "description": "rotate the image",
        "expected": "the image is rotated"
      },
      {
        "description": "close shotwel",
        "expected": "shotwell window disappears"
      },
      {
        "description": "open shotwell",
        "expected": "shotwell opens in \"gallery mode\""
      },
      {
        "description": "search the previously image in the galery",
        "expected": "the image appears rotated"
      },
      {
        "description": "ask shotwell to show the picture in file managed",
        "expected": "nautilus appears with the file selected"
      },
      {
        "description": "double click the file",
        "expected": "Shotwell opens, showing the file not rotated"
      }
    ],
    "post_condition": "",
    "notes": "",
    "bugs": "",
